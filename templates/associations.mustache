 {{{includes.navbar}}}
    <div class="monarch-container">
        <div class="content">
            <div class="wrapperforfooter">
            <div class="jumbotron monarch-disease-light">
                <div id="description">
                    {{#alerts}}
                    <div class="alert">
                      {{{.}}}
                    </div>
                    {{/alerts}}
                    <img id="monarch-logo" class="downloadline" src="/image/logo.png"/>
                    <h1 class="downloadline">{{type}} {{label}}</h1>
                    <a href="{{{download.json}}}" class="btn btn-default downloadline downloadline-left monarch-phenotype-dark">JSON</a>
                    <a class="downloadline btn btn-large btn-default btn-monarch-main monarch-feedback-dark"
                            href="mailto:info@monarchinitiative.org">Your feedback welcome!</a>
                     <p>
                        Source: {{{primary_xref}}}
                    </p>
                </div>
                <div id="categories">
                    <a href="#associations"><div id="initialtab" class="tab contenttab">Direct Associations ({{associationNum}})</div></a>
                    <div class="tabcontainer"> 
  {{#inferredNum}}
  <a href="#inferred"><div class="tab contenttab">Inferred Associations ({{inferredNum}})</div></a>
  {{/inferredNum}}
  {{^inferredNum}}
  <div class="tab greyed-out">Inferred Associations (0)</div>
  {{/inferredNum}}
</div>
                </div>
            </div>
            <div id="complete-info">
            
            <div id="associations" class="first category">
  <div id="r3" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
      
    <!-- Quiet rollover. -->
    <div class="panel panel-default">
      <!-- <div class="panel-heading"> -->
