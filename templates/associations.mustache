      <!-- </div> -->
      <div class="panel-body">

        <div id="results-area" class="row">
                      
          <div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
            <div>
              <h4>Filter results</h4>
        
              <div id="associations-table-filter" class="bs3filter">
                Loading...
             </div>
        
            </div>      
          </div>
        
          <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
          
            <div id="associations-table-pager-top" class="bs3pager">
            </div>
            <div id="associations-table">
              pending...
            </div>
            <div id="associations-table-pager-bottom" class="bs3pager-bottom">
            </div>
          
          </div>

       </div>

      <!-- <div class="clearfix"></div> -->
      </div>
    </div>
  </div>
</div>

