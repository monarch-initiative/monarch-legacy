    {{{includes.navbar}}}
    <div class="monarch-container">
        <div class="content">
            <div class="wrapperforfooter">
            <div class="jumbotron">
                <div id="description">
                    <img id="monarch-logo" class="downloadline" src="/image/logo.png"/>
                    <h1 class="downloadline">Search Results: {{term}}</h1>
                    <a href="{{{download.json}}}" class="downloadline btn btn-info">JSON</a>
                </div>
            </div>
            <div id="complete-info">
                <h3>Exact Match</h3>
                {{{description}}}
                <table>
                    {{{resultsTable}}}
                </table>
                <h3>Site Search</h3>
                <span id="text-search">Results for {{term}} using a general search over all text fields</span>
                <div id="spinner">
                        <h3>Loading...</h3>
                        <div class="cube1"></div>
                        <div class="cube2"></div>
                </div>

            </div>


            </div> <!-- closes wrapperforfooter -->
            {{{includes.footer}}}
        </div>
    </div>

