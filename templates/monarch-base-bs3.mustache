              </ul>
            </li>
	  </ul>

	  {{#monarch_nav_search_p}}
          <ul class="nav navbar-nav navbar-right searchspace">
	    <li>
	      <form id="search_form"
		    class="navbar-form"
		    action="/search"
		    role="search">
		<div class="form-group">
		  <input id="search"
			 type="text"
			 class="form-control"
			 placeholder="Search (e.g. Parkinson's)" />
		</div>
	      </form>
	    </li>
